#include <iostream>
using namespace std;
int main() {
int N;
    cout << "Imput n";
    cin >> N;

    int A[N];
    cout << "Imput elements mas ";
    for(int i=0; i<N; i++) {
        cin >> A[i];
    }

    int i = 0, j = N-1;
    while(i <= j) {
        if(i == j) {
            cout << A[i] << " ";
        }
        else {
            cout << A[i] << " " << A[j] << " ";
        }
        i++;
        j--;
    }

}
    
