#include <iostream>
using namespace std;
int main() {
 int N;
    cout << " N ";
    cin >> N;
	
    int mas[N];
    int value = 1;
    for (int i = 0; i < N; i++) {
        mas[i] = value; 
        value += 2;
    }
	
    cout << "Mas";
    for (int i = 0; i < N; i++) {
        cout << mas[i] << " "; 
    }
}
