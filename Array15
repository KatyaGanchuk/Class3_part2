#include <iostream>
using namespace std;
int main() {
int n;
    cout << "Imput n ";
    cin >> n;

    int a[n];
    cout << "Imput elements mass ";
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
     for (int i = 0; i < n; i += 2) {
        cout << a[i] << " ";
    }
 for (int i = n - 2 + (n % 2); i >= 0; i -= 2) {
        cout << a[i] << " ";
    }
}
